manifest {
    name = 'Phylo-IMD'
    mainScript = 'main.nf'
    version = '1.0'
}

env {
   CACHE_4_TCOFFEE='${TMPDIR:-/tmp}/.tcoffee/cache'
   LOCKDIR_4_TCOFFEE='${TMPDIR:-/tmp}/.tcoffee/lock'
   TMP_4_TCOFFEE='${TMPDIR:-/tmp}/.tcoffee/tmp'
   DUMMY='$(mkdir -p $CACHE_4_TCOFFEE; mkdir -p $LOCKDIR_4_TCOFFEE; mkdir -p $TMP_4_TCOFFEE)'
}